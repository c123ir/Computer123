{
  "name": "computer123-backend",
  "version": "1.0.0",
  "description": "Backend API for Computer123 Form Builder",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "db:deploy": "prisma migrate deploy",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "form-builder",
    "api",
    "postgresql",
    "express"
  ],
  "author": "Computer123",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "prisma": "^6.9.0",
    "@prisma/client": "^6.9.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.5",
    "@types/morgan": "^1.9.9",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
